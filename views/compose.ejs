<%- include("partials/header"); -%>
  <h1 class="text-3xl font-bold text-center my-8 custom-gray-color">Compose</h1>
  <div class="a"></div>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="postTitle">
      <label>Post</label>
      <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-outline-secondary" type="submit" name="button">Publish</button>
  </form>
  <%- include("partials/footer"); -%>