<%- include("partials/header"); -%>
  <h1 class="text-3xl font-bold text-center my-8">Home</h1>
  <p>
    <%= startingContent %>
  </p>


  <% posts.forEach(function(post){ %>

    <h1>
      <%=post.title%>
    </h1>
    <p>
      <%=post.content.substring(0, 100) + " ..." %>
        <a href="/posts/<%=post.title%>">Read More</a>
    </p>


    <% }) %>